{
  "version": 1,
  "paths": {
    "/broadcast": {
      "name": "/broadcast",
      "lambdaFunction": "flowiseBroadcast",
      "permissions": {
        "setting": "open"
      }
    },
    "/detect-language": {
      "name": "/detect-language",
      "lambdaFunction": "detectLanguage",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "guest": [
          "create",
          "read",
          "update"
        ]
      }
    },
    "/users": {
      "name": "/users",
      "lambdaFunction": "handleUsers",
      "permissions": {
        "setting": "open"
      }
    },
    "/transcribe": {
      "name": "/transcribe",
      "lambdaFunction": "transcribeAudio",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "guest": [
          "create",
          "read",
          "update"
        ],
        "groups": {
          "SuperAdmin": [
            "create",
            "read",
            "update"
          ],
          "Admin": [
            "create",
            "read",
            "update"
          ]
        }
      }
    },
    "/scraper/websites": {
      "name": "/scraper/websites",
      "lambdaFunction": "websiteScraping",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "groups": {
          "SuperAdmin": [
            "create",
            "read",
            "update"
          ],
          "Admin": [
            "create",
            "read",
            "update"
          ]
        }
      }
    },
    "/flowise/middleware": {
      "name": "/flowise/middleware",
      "lambdaFunction": "flowiseMiddleware",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "guest": [
          "create",
          "read",
          "update"
        ],
        "groups": {
          "SuperAdmin": [
            "create",
            "read",
            "update"
          ],
          "Admin": [
            "create",
            "read",
            "update"
          ]
        }
      }
    }
  }
}