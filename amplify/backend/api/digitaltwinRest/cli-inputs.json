{
  "version": 1,
  "paths": {
    "/flowise/middleware": {
      "name": "/flowise/middleware",
      "lambdaFunction": "flowiseMiddleware",
      "permissions": {
        "setting": "open"
      }
    },
    "/broadcast": {
      "name": "/broadcast",
      "lambdaFunction": "flowiseBroadcast",
      "permissions": {
        "setting": "open"
      }
    },
    "/detect-language": {
      "name": "/detect-language",
      "lambdaFunction": "detectLanguage",
      "permissions": {
        "setting": "open"
      }
    }
  }
}