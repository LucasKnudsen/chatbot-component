{
  "version": 1,
  "paths": {
    "/broadcast": {
      "name": "/broadcast",
      "lambdaFunction": "flowiseBroadcast",
      "permissions": {
        "setting": "open"
      }
    },
    "/detect-language": {
      "name": "/detect-language",
      "lambdaFunction": "detectLanguage",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "guest": [
          "create",
          "read",
          "update"
        ]
      }
    },
    "/flowise/middleware": {
      "name": "/flowise/middleware",
      "lambdaFunction": "flowiseMiddleware",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update"
        ],
        "guest": [
          "create",
          "read",
          "update"
        ]
      }
    }
  }
}