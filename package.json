{
  "name": "fraia-embed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint-fix": "eslint --fix \"src/**/*.ts*\"",
    "test": "cypress open",
    "test:run": "cypress run",
    "fraia:codegen": "./scripts/fraia-codegen/codegen.sh",
    "allow-backend": "jq '.noUpdateBackend = false' ./amplify/.config/local-env-info.json > tmp.$$.json && mv tmp.$$.json ./amplify/.config/local-env-info.json && cd amplify && mkdir -p backend && echo '{}' > backend/backend-config.json && cd../",
    "quick-push": "git push upstream dev && git checkout testing && git merge dev && git push upstream testing && git checkout staging && git merge testing && git push upstream staging && git checkout dev"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@heygen/streaming-avatar": "^1.0.14",
    "@modular-forms/solid": "^0.20.0",
    "@popperjs/core": "^2.11.8",
    "@solid-primitives/storage": "^4.2.0",
    "@ts-stack/markdown": "^1.5.0",
    "audiomotion-analyzer": "^4.5.0",
    "aws-amplify": "^5.3.12",
    "axios": "^1.6.7",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.7.2",
    "solid-element": "^1.8.0",
    "solid-js": "^1.7.8",
    "solid-popper": "^0.3.0",
    "solid-styled-jsx": "^0.27.1",
    "solid-toast": "^0.5.0",
    "solid-transition-group": "^0.2.3",
    "terracotta": "^1.0.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-amplify/cli-extensibility-helper": "^3.0.24",
    "@tailwindcss/typography": "^0.5.10",
    "@types/aws-lambda": "^8.10.126",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-solid": "0.12.0",
    "json-schema-to-typescript": "^15.0.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-solid": "^2.7.0"
  }
